<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Test Set"
            visible="true"
            id="org.softwarepraktikum.plugin.actionSet">
         <menu
               label="Folding Debug Menu"
               id="foldingDebugMenu">
            <separator
                  name="foldingDebugGroup">
            </separator>
         </menu>
         <action
               class="org.softwarepraktikum.plugin.actions.FoldingDebugger"
               icon="icons/sample.gif"
               id="org.softwarepraktikum.plugin.actions.FoldingDebugger"
               label="&amp;Folding Debug Action"
               menubarPath="foldingDebugMenu/foldingDebugGroup"
               toolbarPath="foldingDebugGroup"
               tooltip="Hello, Eclipse world">
         </action>
      </actionSet>
   </extension>
   
   <extension
    point = "org.eclipse.ui.editors">
	      <editor
	            class="org.softwarepraktikum.plugin.reditor.RegexEditor"
	            id="org.eclipse.ui.examples.contributions.editor"
	            name="Regex C/C++ Editor">
	      </editor>
	</extension>
   
      <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            name="Regex C/C++ Debug Configuration"
            delegate="org.softwarepraktikum.plugin.cdtregexdebugger.FoldingLaunchConfigType"
            modes="debug"
            public="true"
            sourceLocatorId="org.eclipse.cdt.debug.core.sourceLocator"
            sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer"
            id="org.softwarepraktikum.plugin.cdthighlighting.ExampleLaunch">
      </launchConfigurationType>
   </extension>

   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            type="org.softwarepraktikum.plugin.cdthighlighting.ExampleLaunch"
            class="org.softwarepraktikum.plugin.cdtregexdebugger.CDTDebuggerTabGroup"
            id="org.softwarepraktikum.plugin.cdthighlighting.ExampleTabGroup">
      </launchConfigurationTabGroup>
   </extension>
   
   <extension 
         point="org.eclipse.cdt.debug.core.CDebugger">
      <debugger
            platform="*"
            name="Regex C/C++ Debugger"
            modes="run"
            cpu="*"
            class="org.softwarepraktikum.plugin.cdtregexdebugger.CDTFoldingDebugger"
            id="org.softwarepraktikum.plugin.cdthighlighting.ExampleDebugger">
      </debugger>
   </extension>   
   
   <extension
         point="org.eclipse.cdt.ui.foldingStructureProviders">
      <provider
            class="org.softwarepraktikum.plugin.cdtfolding.CDTFolderStructureProvider"
            id="Regex C/C++ Folding"
            name="Regex C/C++ Folding"
            preferencesClass="org.softwarepraktikum.plugin.cdtfolding.CDTFoldingPreferenceBlock">
      </provider>
   </extension>
</plugin>